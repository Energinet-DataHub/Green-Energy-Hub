{
    "$id": "https://github.com/green-energy-hub/schemas/change-of-supplier",
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "Change of supplier",
    "type": "object",
    "additionalProperties": false,
    "properties": {
        "BalanceResponsibleParty": { "$ref": "https://github.com/green-energy-hub/schemas#/definitions/marketParticipant" },
        "EnergySupplier": { "$ref": "https://github.com/green-energy-hub/schemas#/definitions/marketParticipant"},
        "Consumer": { "$ref": "https://github.com/green-energy-hub/schemas#/definitions/marketParticipant"},
        "MarketEvaluationPoint": { "$ref": "https://github.com/green-energy-hub/schemas#/definitions/marketEvaluationPoint" },
        "StartDate": { 
            "title": "Start of occurrence",
            "description": "Date in UTC that defines the date when a new energysupplier is responsible for the meteringpoint",
            "$ref": "https://github.com/green-energy-hub/schemas#/definitions/utc_date" 
        },
        "Transaction": { "$ref": "https://github.com/green-energy-hub/schemas#/definitions/referenceId" }
    },
    "required": [ "BalanceResponsibleParty", "EnergySupplier", "Consumer", "MarketEvaluationPoint", "StartDate", "Transaction" ]
}